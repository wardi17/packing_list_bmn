--1.langkah  pertama di database um_db

INSERT INTO an_user
select 'wardi' as email ,'BMN' as  cabang,'wardi' as id_cust, 1 as pass, 'Wardi' as nama, 
poin_a,poin_b,status_pass,divisi_old,divisi_new,gender,'Staff' as jabatan, nik,status,code_menu
from an_user where email='herman'


UPDATE an_user SET  email='wardi'   where nama='Wardi'
select * from an_user where nama='Wardi'

